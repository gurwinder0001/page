      <script type="text/javascript" src="//code.jquery.com/jquery-1.11.0.min.js"></script>
      <script>
  $('.tab-template--22518065103142__783e0a15-bf3d-49c0-af6c-7d261324e01c-js').each(function(){
    $(this).find('.dt-sc-tabs-content-Details:first').addClass('active');
  $(this).find('.tablinks:first').addClass('active');
  });

  $('.tab-template--22518065103142__783e0a15-bf3d-49c0-af6c-7d261324e01c-js .tablinks').each(function(){
    $(this).on('click', function(){
    var $this = $(this);
    var $theTab = $(this).attr('id');
    $(this).fadeIn('slow').siblings(".dt-sc-tabs-content-Details").hide();
    if($this.hasClass('active')){
      // do nothing

    } else {
     $this.closest('.tab-template--22518065103142__783e0a15-bf3d-49c0-af6c-7d261324e01c-js').find('.tabs .tablinks, .tabs_container .dt-sc-tabs-content-Details').removeClass('active'); $('.tablinks[id="'+$theTab+'"]').addClass('active');
     $('.tab-template--22518065103142__783e0a15-bf3d-49c0-af6c-7d261324e01c-js .tabs_container .dt-sc-tabs-content-Details[data-tab="'+$theTab+'"], .tab-template--22518065103142__783e0a15-bf3d-49c0-af6c-7d261324e01c-js .tabs .tablinks[id="'+$theTab+'"]').addClass('active');
       }
     });
    });
</script>
